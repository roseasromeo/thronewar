<div class='nested-tools'>
    <tr>
      <% if f.name == "" || f.name == nil %>
        <td><%= link_to "Tool", character_system_tool_path(@character_system,f) %></td>
      <% else %>
        <td><%= link_to f.name, character_system_tool_path(@character_system,f) %></td>
      <% end %>
      <td><%= f.points %></td>
    </tr>
</div>
