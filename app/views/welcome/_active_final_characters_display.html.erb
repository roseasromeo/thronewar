<% if !@active_final_characters.empty? %>
  <div class="active_final_characters">
    <h3>Active Characters</h3>
    <table>
        <tr>
          <th>Name</th>
          <th>Throne War</th>
        </tr>
        <% @active_final_characters.each do |final_character| %>
          <%= render 'final_character_fields', final_character: final_character %>
        <% end %>
    </table>
  </div>
<% end %>
