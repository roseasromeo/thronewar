<div class='nested-dependencies'%>
    <% dep_ability = f.depends_on %>
    <h2 class='ability_name'><%= dep_ability.name %></h2>
    <table class='ability'>
        <td class='sidebar'><%= dep_ability.level %><%= dep_ability.automatic %></td>
        <td class='body'><%= sanitize dep_ability.short_text.html_safe, :tags => %w(p strong b em i u table tr th td a ul ol li), :attributes => %w(href) %></td>
      </tr>
    </table>
</div>
